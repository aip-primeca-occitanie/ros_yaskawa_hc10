<!--
  Manipulator specific version of 'robot_interface_streaming.launch'.

  Defaults provided for HC10:
   - 6 joints

  Usage:
    robot_interface_streaming_hc10.launch robot_ip:=<value> controller:=<yrc1000>
-->
<launch>
  <arg name="robot_ip" />

  <!-- controller: Controller name (yrc1000) -->
  <arg name="controller"/>

  <rosparam command="load" file="/home/yaska/catkin_ws/src/ros_yaskawa_hc10/motoman_hc10_support/config/joint_names_hc10.yaml" />

  <include file="/home/yaska/catkin_ws/src/ros_yaskawa_hc10/motoman_driver/launch/robot_interface_streaming_$(arg controller).launch">
    <arg name="robot_ip" value="$(arg robot_ip)" />
  </include>
</launch>
